<TelaPessoas>
    name: "pessoas"

    MDBoxLayout:
        id: tela
        orientation: 'vertical'
        MDTopAppBar:
            icon: "account-badge"
            elevation: 0
            type: "top"
            right_action_items:[["home", lambda x: app.set_peesoa_off(), "Home"], ["account-badge", lambda x: root.tela_pessoas(), "Pessoas Online"],["account-group", lambda x: root.tela_grupos(), "Grupos"]]

        ScrollView:
            id: telas
            MDList:
                id: listagem